{
  "name": "react-mui-ch5",
  "version": "0.1.0",
  "private": true,
  "homepage": "/ReactCH5/",
  "dependencies": {
    "@crestron/ch5-crcomlib": "^1.2.0",
    "@crestron/ch5-utilities-cli": "^0.1.52",
    "@crestron/ch5-webxpanel": "^1.2.5",
    "@emotion/react": "^11.7.1",
    "@emotion/styled": "^11.6.0",
    "@fontsource/roboto": "^4.5.2",
    "@fortawesome/fontawesome-svg-core": "^1.3.0",
    "@fortawesome/pro-light-svg-icons": "^6.0.0",
    "@fortawesome/pro-duotone-svg-icons": "^6.0.0",
    "@fortawesome/pro-regular-svg-icons": "^6.0.0",
    "@fortawesome/pro-solid-svg-icons": "^6.0.0",
    "@fortawesome/pro-thin-svg-icons": "^6.0.0",
    "@fortawesome/react-fontawesome": "^0.1.17",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@testing-library/user-event": "^13.2.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "5.0.0",
    "sass": "^1.45.1",
    "web-vitals": "^2.1.0",
    "@mui/icons-material": "^5.2.5",
    "@mui/material": "^5.2.5",
    "@mui/styles": "^5.2.3",
    "@reduxjs/toolkit": "^1.8.0",
    "react-router-dom": "6"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "build:archive": "yarn build && ch5-cli archive -p ReactCH5 -d build -o build ",
    "build:deploy": "ch5-cli deploy -H tshostname -t touchscreen dist/ReactCH5.ch5z",
    "build:deploy:mobile": "ch5-cli deploy -H tshostname -t mobile dist/ReactCH5.ch5z",
    "build:deployweb": "ch5-cli deploy -H 192.168.2.29 -p -t web build/ReactCH5.ch5z"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
